	"version": "0.1.1",
	"type": "module",
	"scripts": {
		"setup": "node scripts/reposetup.js",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
